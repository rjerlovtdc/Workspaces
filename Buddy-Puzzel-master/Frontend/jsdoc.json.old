{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "includePattern": "\\.js$",
    "excludePattern": "(node_modules/|docs|Sys|Config)"
  },
  "plugins": [ "plugins/markdown" ],
  "opts": {
    "encoding": "utf8",
    "destination": "../DOCS/",
    "recurse": true,
    "verbose": true,
    "template": "node_modules/docdash"
  },
  "docdash": {
    "static": true,
    "sort": true,
    "search": true,
    "sectionOrder": [
      "Classes",
      "Modules",
      "Externals",
      "Events",
      "Namespaces",
      "Mixins",
      "Tutorials",
      "Interfaces"
    ],
    "meta": {
      "title": "BUDDY",
      "description": "3.5.0.0",
      "lastUpdated": "02-01-2025"
    }
  }
}
