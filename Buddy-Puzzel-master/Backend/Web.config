<configuration>
  <configSections>
    <section name="NetDesign" type="System.Configuration.NameValueSectionHandler" />
  </configSections>
  <NetDesign>
    <add key="CacheFolder" value="C:\Temp\Cache" />
    <add key="CacheTimer" value="8" />
  </NetDesign>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <!-- SeriLog :: Verbose, Debug, Information, Warning, Error, Fatal-->
    <add key="serilog:enrich:with-property:Environment" value="PuzzelBackend" />
    <add key="serilog:minimum-level" value="Information" />
    <add key="serilog:using:File" value="Serilog.Sinks.File" />
    <add key="serilog:write-to:File.path" value="C:\Temp\Logs\PuzzelBackend-.txt" />
    <add key="serilog:write-to:File.rollingInterval" value="Day" />
    <add key="serilog:write-to:File.retainedFileCountLimit" value="30" />
    <add key="serilog:using:Seq" value="Serilog.Sinks.Seq" />
    <add key="serilog:write-to:Seq.serverUrl" value="http://localhost:5341" />
  </appSettings>
  <connectionStrings>
    <clear />
    <add name="buddyCS" connectionString="Data Source=(local);Initial Catalog=Buddy-DB;Persist Security Info=True;User ID=tdcbuddy;Pooling=False;Connect Timeout=5;Encrypt=True;TrustServerCertificate=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
